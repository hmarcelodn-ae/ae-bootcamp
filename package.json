{
   "name": "agileengine-bootcamp",
   "version": "1.0.0",
   "description": "AgileEngine Bootcamp",
   "main": "index.js",
   "scripts": {
      "commit": "git-cz",
      "start:dev": "nodemon src/server.ts && ts-node src/server.ts",
      "start": "ts-node src/index.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
   },
   "husky": {
      "hooks": {
         "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
         "pre-commit": "npm run lint"
      }
   },
   "config": {
      "commitizen": {
         "path": "cz-conventional-changelog"
      }
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/hmarcelodn-ae/agileengine-bootcamp.git"
   },
   "keywords": [],
   "author": "Hugo Marcelo Del Negro",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/hmarcelodn-ae/agileengine-bootcamp/issues"
   },
   "homepage": "https://github.com/hmarcelodn-ae/agileengine-bootcamp#readme",
   "dependencies": {
      "compression": "^1.7.4",
      "cookie-parser": "^1.4.5",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "express-async-errors": "^3.1.1",
      "express-validator": "^6.12.2",
      "mysql": "^2.14.1",
      "pg": "^8.7.1",
      "reflect-metadata": "^0.1.10",
      "tsconfig-paths": "^3.11.0",
      "typedi": "^0.10.0",
      "typeorm": "0.2.37"
   },
   "devDependencies": {
      "@commitlint/cli": "^13.2.0",
      "@commitlint/config-conventional": "^13.2.0",
      "@commitlint/prompt-cli": "^13.2.0",
      "@types/compression": "^1.7.2",
      "@types/cookie-parser": "^1.4.2",
      "@types/cors": "^2.8.12",
      "@types/express": "^4.17.13",
      "@types/express-validator": "^3.0.0",
      "@types/node": "^8.0.29",
      "cz-conventional-changelog": "^3.3.0",
      "git-cz": "^4.7.6",
      "husky": "^7.0.2",
      "nodemon": "^2.0.13",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   }
}
